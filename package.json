{
  "name": "node-bluetooth",
  "version": "1.2.6",
  "description": "Bluetooth serial port communication for Node.js",
  "keywords": [
    "bluetooth"
  ],
  "main": "index.js",
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "dependencies": {
    "bindings": "^1.5.0",
    "bluetooth-serial-port": "git+https://github.com/tinyprinter/node-bluetooth-serial-port.git"
  },
  "engines": {
    "node": ">= 12.4.x",
    "npm": ">= 6.14.x"
  },
  "scripts": {
    "rebuild": "electron-rebuild -f -w bluetooth-serial-port",
    "install": "npm rebuild && node ../copyfiles/copyfiles -f ../bluetooth-serial-port/build/Release/BluetoothSerialPort.node Release"
  },
  "license": "MIT",
  "author": "lsong",
  "directories": {
    "example": "example"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "electron-rebuild": "^2.3.5",
    "prebuild": "^10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/song940/node-bluetooth.git"
  },
  "bugs": {
    "url": "https://github.com/song940/node-bluetooth/issues"
  },
  "homepage": "https://github.com/song940/node-bluetooth#readme"
}
